    "bootstrap": "^4.5.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "moment": "^2.27.0",
    "popper.js": "^1.16.1",
    "spdy": "^4.0.2",
    "underscore": "^1.11.0"
  }
